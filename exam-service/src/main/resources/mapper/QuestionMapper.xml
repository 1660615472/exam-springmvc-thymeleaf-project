<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cheer.exam.dao.QuestionMapper">
    <insert id="insertQuestion">
    insert into question values(#{no},#{question},#{a},#{b},#{c},#{d},#{answer});
</insert>
    <select id="getQuestionList" resultType="com.cheer.exam.model.Question">
    select * from question;
</select>
    <select id="getKey" resultType="java.lang.String">
        SELECT answer FROM question;
    </select>

    <update id="update"></update>
</mapper>